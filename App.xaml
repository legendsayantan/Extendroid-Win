<?xml version="1.0" encoding="utf-8"?>
<Application
    x:Class="Extendroid.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Extendroid">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="white">#FFFFFFFF</Color>
            <Color x:Key="theme0">#F3E5F5</Color>
            <Color x:Key="theme1">#E2BBE9</Color>
            <Color x:Key="theme2">#9B86BD</Color>
            <Color x:Key="theme3">#7776B3</Color>
            <Color x:Key="theme4">#5A639C</Color>
            <Color x:Key="theme5">#3E4A89</Color>
            <Color x:Key="black">#FF000000</Color>
            <Color x:Key="red">#FF0505</Color>

            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->
        </ResourceDictionary>
    </Application.Resources>
</Application>
